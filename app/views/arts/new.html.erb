
  <div class="background" style="background-image: linear-gradient(-225deg, rgba(0,0,0,0) 0%, rgba(0,0,0,0) 50%), url('https://ohsooclean.com/wp-content/uploads/2018/06/testimonials.jpg');">
    <div class="container">
      <div class="art-card">
        <div>
          <div>
            <h3>Your art:</h3>
          </div>
          <%= simple_form_for @art do |f| %>
          <div>
            <%= f.input :title, required: false,
            class: "com-title",
            placeholder: "Title..." %>
          </div>

          <div class="art-p-s">
            <div>
              <%= f.input :photo %>
              <%= f.input :photo_cache, as: :hidden %>
            </div>
            <div class="art-sub-btn">
              <%= f.button :submit %>
            </div>
          </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>


