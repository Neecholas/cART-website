<div class="background-commission" style="background-image: linear-gradient(-225deg, rgba(0,0,0,0) 0%, rgba(0,0,0,0) 50%), url('https://images.unsplash.com/photo-1503289130890-6eff9c5df553?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=7d4ed6009e8a0e8b6948ebe612d5827f&auto=format&fit=crop&w=1050&q=80');">
  <div class="com-container">
    <div class="com-card">
      <div>
        <h3>Log in</h3>
      </div>
       <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="form-inputs">
        <%= f.input :email,
                    required: false,
                    autofocus: true,
                    input_html: { autocomplete: "email" } %>
        <%= f.input :password,
                    required: false,
                    input_html: { autocomplete: "current-password" } %>
      </div>
      <div class="p-s">
        <div class="sub-btn">
          <%= f.button :submit, "Log in" %>
        </div>
        <% end %>
      </div>
      <%= render "devise/shared/links" %>
    </div>
  </div>
</div>

