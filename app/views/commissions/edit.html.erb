<h2>Edit Commission</h2>

  <%= form_for(@commission) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.label :amount %>
    <%= f.text_field :amount %>
    <%= f.label :photo %>
    <%= f.text_field :photo %>
    <%= f.submit %>
  <% end %>
